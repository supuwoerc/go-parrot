{
    "swagger": "2.0",
    "info": {
        "description": "使用golang开发后台管理系统",
        "title": "Go-Parrot鹦鹉学舌",
        "contact": {},
        "version": "1.0.0"
    },
    "paths": {
        "/api/v1/public/user/login": {
            "post": {
                "description": "用户登录接口",
                "tags": [
                    "用户管理"
                ],
                "summary": "用户登录",
                "parameters": [
                    {
                        "type": "string",
                        "description": "用户名",
                        "name": "name",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "密码",
                        "name": "password",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "登录成功",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "登录失败",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}